    "classnames": "*",
    "react": "*",
    "react-dom": "*",
    "antd": "*",
    "@ant-design/icons": "*",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "dayjs": "^1.11.11"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/react": "^18.2.60",
